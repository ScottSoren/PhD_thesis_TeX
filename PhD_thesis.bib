isbn = {2211-2855},
issn = {22112855},
journal = {Nano Energy},
keywords = {Density functional theory,Electrocatalysis,Oxygen evolution,Oxygen reduction,Volcano},
pages = {126--135},
publisher = {Elsevier},
title = {{Beyond the top of the volcano? A unified approach to electrocatalytic oxygen reduction and oxygen evolution}},
url = {http://dx.doi.org/10.1016/j.nanoen.2016.04.011},
volume = {29},
year = {2016}
