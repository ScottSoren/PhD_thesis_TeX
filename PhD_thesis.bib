journal = {Journal of Air Transport Management},
keywords = {Airlines,Atmospheric carbon dioxide concentrations,Carbon budget,Carbon offset,Emissions pathway,Targets},
pages = {71--83},
title = {{What role for offsetting aviation greenhouse gas emissions in a deep-cut carbon world?}},
volume = {63},
year = {2017}
