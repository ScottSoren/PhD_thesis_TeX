keywords = {Corrosion,Electrochemistry,Heterogeneous catalysis,Thin films,Water splitting},
number = {12},
pages = {2075--2081},
title = {{Benchmarking the Stability of Oxygen Evolution Reaction Catalysts: The Importance of Monitoring Mass Losses}},
