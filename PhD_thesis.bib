issn = {13882481},
journal = {Electrochemistry Communications},
number = {10},
pages = {1865--1868},
publisher = {Elsevier B.V.},
title = {{Oxygen evolution on nanocrystalline RuO{\textless}sub{\textgreater}2{\textless}/sub{\textgreater} and Ru{\textless}sub{\textgreater}0.9{\textless}/sub{\textgreater}Ni{\textless}sub{\textgreater}0.1{\textless}/sub{\textgreater}O{\textless}sub{\textgreater}2-$\delta${\textless}/sub{\textgreater} electrodes - DEMS approach to reaction mechanism determination}},
volume = {11},
