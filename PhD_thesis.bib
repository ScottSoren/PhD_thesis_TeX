issn = {17545706},
journal = {Energy and Environmental Science},
number = {3},
pages = {1055--1067},
title = {{Towards an atomistic understanding of electrocatalytic partial hydrocarbon oxidation: Propene on palladium}},
volume = {12},
year = {2019}
}
