isbn = {0002-7863},
issn = {1520-5126},
journal = {J. Am. Chem. Soc.},
number = {14},
pages = {16501--16509},
pmid = {20977209},
title = {{Mechanistic Studies of the Oxygen Evolution Reaction by a Cobalt-Phosphate Catalyst at Neutral pH}},
volume = {132},
year = {2010}
}
